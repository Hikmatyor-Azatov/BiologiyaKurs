    <body>
        <!-- Top side of the website -->
        <div
            class="
                website-top
                d-flex
                justify-content-between
                align-items-center
            "
        >
            <a href="#" id="logo" style="font-family: amperzand"
                >Amonov Sarvarbek Biologiya Kursi</a
